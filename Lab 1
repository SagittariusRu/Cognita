# Lab 1

import math

"""x = float(input("x = "))
x = float(input("x = "))
x = float(input("x = "))"""

x = float(-4.5)
y = float(0.75 * math.pow(10, -4))
z = float(0.845 * math.pow(10, 2))

a = math.pow(8 + math.pow(math.fabs(x - y), 2) + 1, 1 / 3)
b = a / (math.pow(x, 2) + math.pow(y, 2) + 2)
u = b - (math.pow(math.e, math.fabs(x - y)) * math.pow(math.tan(z) * math.tan(z) + 1, x))

print("u = {0:4f}".format(u))
